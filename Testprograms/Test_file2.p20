STARTPROGRAM TEST2;
DATA INT_4 _AAA00, _BBB11, _CCC22, _MAXM;
STARTBLOCK
	READ (_AAA00);
	READ (_BBB11);
	READ (_CCC22);

	IF _AAA00 LT _BBB11 AND _AAA00 LT _CCC22 {
		_MAXM :> _AAA00;
	} ELSE IF _BBB11 LT _AAA00 AND _BBB11 LT _CCC22 {
		_MAXM :> _BBB11;
	} ELSE {
		_MAXM :> _CCC22;
	}
	WRITE (_MAXM);
  
	IF (_AAA00 EQ _BBB11) AND (_AAA00 EQ _CCC22) AND (_BBB11 EQ _CCC22) {
		WRITE (1);
	} ELSE {
		WRITE (0);
	}

	IF NOT (_AAA00 LT 0) OR NOT (_BBB11 LT 0) OR NOT (_CCC22 LT 0) {
		WRITE (-1);
	} ELSE {
		WRITE (0);
	}
	
	IF (_AAA00 LT (_BBB11 ADD _CCC22) ) {
		WRITE(10);
	} ELSE {
		WRITE (0);
	}
ENDBLOCK
