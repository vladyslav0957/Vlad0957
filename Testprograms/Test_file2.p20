STARTPROGRAM TEST2;
DATA INT_4 _AAA00, _BBB11, _CCC22, _MAXM;
STARTBLOCK
	READ (_AAA00);
	READ (_BBB11);
	READ (_CCC22);

	IF _AAA00 GT _BBB11 AND _AAA00 GT _CCC22 {
		_AAA00 :> _MAXM;
	} ELSE IF _BBB11 GT _AAA00 AND _BBB11 GT _CCC22 {
		_BBB11 :> _MAXM;
	} ELSE {
		_CCC22 :> _MAXM;
	}
	WRITE (_MAXM);
  
	IF (_AAA00 EQ _BBB11) AND (_AAA00 EQ _CCC22) AND (_BBB11 EQ _CCC22) {
		WRITE (1);
	} ELSE {
		WRITE (0);
	}

	IF (_AAA00 LT 0) OR (_BBB11 LT 0) OR (_CCC22 LT 0) {
		WRITE (-1);
	} ELSE {
		WRITE (0);
	}
	
	IF NOT (_AAA00 LT (_BBB11 ADD _CCC22) ) {
		WRITE(10);
	} ELSE {
		WRITE (0);
	}
ENDBLOCK
